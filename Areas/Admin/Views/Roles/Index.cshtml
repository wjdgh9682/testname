@model IEnumerable<AppUser>
<!--model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>-->

@{
    ViewData["Title"] = "Roles";
}

<h2>Roles</h2>

<p>
    <a asp-action="Create">Create New User</a>
</p>
<table>
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Role
            </th>
            <th>
                Users
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
                <td></td>
                <td>
                    <a asp-action="Edit" asp-route-id="@role.Id">Edit</a> |
                    <a class="DeletionConfirm" asp-action="Delete" asp-route-id="@role.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


}